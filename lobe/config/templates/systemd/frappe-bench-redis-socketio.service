[Unit]
Description="{{ lobe_name }}-redis-socketio"
PartOf={{ lobe_name }}-redis.target

[Service]
User={{ user }}
Group={{ user }}
Restart=always
ExecStart={{ redis_server }} {{ redis_socketio_config }}
StandardOutput=file:{{ lobe_dir }}/logs/redis-socketio.log
StandardError=file:{{ lobe_dir }}/logs/redis-socketio.error.log
WorkingDirectory={{ sites_dir }}
